SEAL_UPGRADE_CAP_ID=your_seal_upgrade_cap_id
SEAL_KEY_SERVER_1_OBJECT_ID=your_key_server_1_object_id
SEAL_KEY_SERVER_1_URL=https://seal-key-server-testnet-1.mystenlabs.com
SEAL_KEY_SERVER_2_OBJECT_ID=your_key_server_2_object_id
SEAL_KEY_SERVER_2_URL=https://seal-key-server-testnet-2.mystenlabs.com

# Walrus Storage
WALRUS_AGGREGATOR=https://aggregator-devnet.walrus.space
WALRUS_PUBLISHER=https://publisher-devnet.walrus.space
