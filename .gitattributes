# Override language detection for build files and dependencies
target/* linguist-generated=true
node_modules/* linguist-vendored=true
*.rlib linguist-generated=true
*.wasm linguist-vendored=true
*.d.ts linguist-generated=true

# Ensure proper detection of source files
*.ts linguist-language=TypeScript
*.tsx linguist-language=TypeScript
*.rs linguist-language=Rust
*.move linguist-language=Move

# Documentation and config files
*.md linguist-documentation=true
*.json linguist-language=JSON
*.toml linguist-language=TOML
*.yaml linguist-language=YAML
*.yml linguist-language=YAML